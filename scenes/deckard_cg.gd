# Called when the node enters the scene tree for the first time.
func _ready():
	unlock_deckard()

func _on_page_1_btn_pressed():
	page_1.show()
	page_2.hide()
	page_3.hide()
	page_4.hide()
	page_5.hide()
	page_6.hide()
	
func _on_page_2_btn_pressed():
	page_1.hide()
	page_2.show()
	page_3.hide()
	page_4.hide()
	page_5.hide()
	page_6.hide()

func _on_page_3_btn_pressed():
	page_1.hide()
	page_2.hide()
	page_3.show()
	page_4.hide()
	page_5.hide()
	page_6.hide()

func _on_page_4_btn_pressed():
	page_1.hide()
	page_2.hide()
	page_3.hide()
	page_4.show()
	page_5.hide()
	page_6.hide()

func _on_page_5_btn_pressed():
	page_1.hide()
	page_2.hide()
	page_3.hide()
	page_4.hide()
	page_5.show()
	page_6.hide()
	
func _on_page_6_btn_pressed():
	page_1.hide()
	page_2.hide()
	page_3.hide()
	page_4.hide()
	page_5.hide()
	page_6.show()

func _on_back_button_pressed():
	get_tree().change_scene_to_file("res://scenes/image_gallery.tscn")

func _on_image_viewer_gui_input(event):
	if Input.is_mouse_button_pressed(MOUSE_BUTTON_RIGHT):
		image_viewer.hide()
		image_viewer.texture = null

